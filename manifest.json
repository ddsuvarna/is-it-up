{
    "manifest_version": 2,
    "name": "Is it up?",
    "version": "0.4",
    "icons": {
        "16": "./icons/icon.png",
        "48": "./icons/icon.png",
        "128": "./icons/icon.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "./icons/icon.png"
    },
    "permissions": [
        "webRequest",
        "http:///",
        "https:///",
        "notifications"
    ],
    "background": {
        "scripts": [
            "./js/background.js"
        ]
    },
    "options_page": "options.html"
}